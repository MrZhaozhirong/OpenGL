<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pixel.opengl">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.HomeActivity"
            android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.particles.opengl.ParticlesActivity"
            android:screenOrientation="sensor"/>

        <activity android:name="com.pixel.opengl.OpenGLActivity"
            android:screenOrientation="sensor"/>

        <activity android:name="com.earth.opengl.EarthActivity"
                  android:screenOrientation="sensor"/>

        <activity android:name="com.earth.opengl.BowlActivity"
                  android:screenOrientation="sensor"/>

    </application>

</manifest>
